'scopeName':'source.quill',
'name':'Quill',
'fileTypes':['qyl', 'quill'],
'patterns':[
    {
        'match':'".*?"',
        'name':'constant.string.quill',
    },
    {
        'match':'(true|false)',
        'name':'constant.boolean.quill',
    },
    {
        'match':'-?[0-9]+(\\.[0-9]+)?',
        'name':'constant.numeric.quill',
    },
    {
        'match':':?[^ \\t\\n(){}".,:\\]\\[]+(\\.[^ \\t\\n(){}".,:+\\]\\[]+)*',
        'name':'entity.name.quill',
    }
]
